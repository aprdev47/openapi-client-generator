{
    "name": "emulgeator/openapi-client-generator",
    "description": "A tool to generate a PHP client out of an openapi documentation",
    "minimum-stability": "stable",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "nesbot/carbon": "^2.53",
        "emulgeator/enum": "^1.0",
        "emulgeator/array-to-class-mapper": "^0.1.2",
        "guzzlehttp/guzzle": "^7.4",
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "autoload": {
      "psr-4": {
        "Emul\\OpenApiClientGenerator\\": "src"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Emul\\OpenApiClientGenerator\\Test\\": "test"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    }
}
