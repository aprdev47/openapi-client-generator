{
    "name": "szemul/openapi-client-generator",
    "description": "A tool to generate a PHP client out of an openapi documentation",
    "minimum-stability": "stable",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "ext-iconv": "*",
        "ext-json": "*",
        "emulgeator/enum": "^1.0",
        "emulgeator/array-to-class-mapper": "^0.1|^1.0",
        "friendsofphp/php-cs-fixer": "^3.9",
        "psr/container": "^1.1",
        "php-di/php-di": "^6.4",
        "symfony/console": "^6.4",
        "symfony/yaml": "^6.4",
        "nesbot/carbon": "^3.10"
    },
    "autoload": {
      "psr-4": {
        "Emul\\OpenApiClientGenerator\\": "src"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Emul\\OpenApiClientGenerator\\Test\\": "test"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6",
        "psr/http-message": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.1"
    },
    "config" : {
        "platform": {
            "php": "8.4.12"
        }
    }
}
